{
  "dependencies": {
    "docsify": "^4.9.4",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "cd frontend && vue-cli-service lint --fix",
      "git add ."
    ]
  }
}
